import java.util.ArrayList;
import java.util.List;

public class SLL<T> {
	List<T> nodes;
	Node head;
	Node tail;

	SLL() {
		nodes = new ArrayList<T>();
		this.head = this.tail = null;
	}

	private static class Node<T> {
		T data;
		Node next;

		Node(T data, Node next) {
			this.data = data;
			this.next = next;
		}

		@Override
		public String toString() {
			return this.data.toString();
		}
	}
}