
public class DynamicArray {
	int[] staticArray;
	int capacity = 2;
	int length = 0;
	
	DynamicArray(){
		staticArray = new int[this.capacity];
	}
	
	void add(int x) {
		if(length>=capacity) {
			
		}else {
			staticArray[length++]=x;
		}
	}
	
	public static void main(String args[]) {
		DynamicArray obj = new DynamicArray();
	}
}
